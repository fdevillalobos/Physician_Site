<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
        <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
        <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div>
    <%= link_to t('.new', :default => t("helpers.links.new")),
                new_user_registration_path,
                :class => 'btn btn-primary' %>
  </div>
</div>